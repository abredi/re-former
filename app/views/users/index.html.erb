<h1>Hello from home</h1>
<% flash.each do |key, value| %>
  <%= content_tag :div, value, class: "flash #{key}" %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_path(user), class: 'list' %>
      <%= user.email %>
      <%= link_to "edit", edit_user_path(user), class: 'edit' %>
    </li>
  <% end %>
</ul>